<div class="form-group">
  <div class="control-label col-sm-2"><%= I18n.t("batch_action_types.functional_move.location") %></div>
  <% if form.readonly? %>
    <div class="label-only col-sm-6">
      <%= I18n.t("enumerations.runcorn_location.#{form.obj['location']}") %>
    </div>
  <% else %>
    <div class="col-sm-6">
      <%= form.select "location", jsonmodel_definition(:movement).options_for(form, "functional_location", false), :default => 'HOME' %>
    </div>
  <% end %>
</div>
