<% render_aspace_partial :partial => "external_ids/template" %>

<section id="<%= section_id %>" class="subrecord-form-dummy">
  <h3><%= I18n.t("external_id._plural") %></h3>
  <% external_ids.each do |external_id| %>
    <div class="subrecord-form-container">
      <% context.push("external_id", external_id) do |context| %>
        <% context.emit_template("external_id") %>
      <% end %>
    </div>
  <% end %>
</section>
